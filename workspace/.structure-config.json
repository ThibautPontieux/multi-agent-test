{
  "workspace": {
    "version": "1.0.0",
    "organization": {
      "enabled": true,
      "autoValidation": true,
      "enforcementLevel": "warning"
    },
    "structure": {
      "maxDepth": 5,
      "namingConvention": "kebab-case",
      "requiredDirectories": [
        "projects",
        "templates",
        "shared",
        ".system"
      ]
    },
    "templates": {
      "react-app": {
        "directories": [
          "src/components",
          "src/services",
          "src/utils",
          "src/types",
          "tests/unit",
          "tests/integration",
          "docs/specifications",
          "docs/designs",
          ".agents"
        ],
        "conventions": {
          "components": "PascalCase",
          "files": "kebab-case",
          "directories": "kebab-case"
        }
      }
    },
    "agents": {
      "responsibilities": {
        "designer": ["docs/", "specifications/", "designs/", "wireframes/"],
        "developer": ["src/", "tests/", "package.json", "config/"],
        "qa": ["tests/", "reports/", "validation/"],
        "orchestrator": [".system/", "workflows/", "logs/"]
      }
    }
  }
}